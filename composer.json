{
    "name": "uicklv/laravel-monobank",
    "description": "Laravel package for Monobank",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "^11.43|^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Uicklv\\LaravelMonobank\\": "src/",
            "Uicklv\\LaravelMonobank\\tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uicklv\\LaravelMonobank\\Providers\\MonobankServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Topor Mykola",
            "email": "m.topor@adtelligent.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.11"
    },
    "autoload-dev": {
        "Uicklv\\LaravelMonobank\\tests\\": "tests/"
    }
}
